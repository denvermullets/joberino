%div.container
  %div.row
    %div.col
    %div.col-6
      %br
      - if current_user && current_user.id == 1
        %div
          = form_tag({action: :new_jobs}, multipart: true) do
            = file_field_tag 'json'
            = submit_tag 'Submit'

        - if job_count && all_users
          %br

          %p= "Jobs listed: #{job_count}"
          %p= "Users: #{all_users.count}"
          %p= "Job Actions: #{job_actions}"
          - all_users.each do |user|
            = "#{user.id} - #{user.email} - #{user.created_at.in_time_zone('EST').strftime("%B %d, %Y")}"
            %br
    %div.col